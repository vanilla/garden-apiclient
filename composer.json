{
	"name": "vanilla/garden-http",
	"description": "An unbloated HTTP client library for building RESTful API clients.",
	"license": "MIT",
	"authors": [
		{
			"name": "Todd Burry",
			"email": "todd@vanillaforums.com"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"lib-curl": "*"
	},
	"config": {
		"platform": {
			"php": "7.0.26"
		},
		"fxp-asset": {
			"vcs-driver-options": {
				"github-no-api": true
			}
		}
	},
	"require-dev": {
		"vanilla/garden": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Garden\\Http\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garden\\Http\\Tests\\": "tests"
		}
	}
}
