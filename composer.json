{
	"name": "vanilla/garden-http",
	"description": "An unbloated HTTP client library for building RESTful API clients.",
	"license": "MIT",
	"authors": [
		{
			"name": "Todd Burry",
			"email": "todd@vanillaforums.com"
		}
	],
	"require": {
		"php": ">=7.4",
		"ext-curl": "*",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"slim/psr7": "^1.6",
		"slim/slim": "^4.11",
		"slim/http": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"Garden\\Http\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garden\\Http\\Tests\\": "tests"
		}
	}
}
